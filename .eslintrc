{
  "env": {
    "commonjs": true,
    "node": true,
    "es6":true
  },
  "plugins": [
    "react"
  ],
  "parser": "babel-eslint",
  "ecmaVersion": 6,
  "ecmaFeatures": {
    "jsx": true
  },
  "rules": {
    "no-var": 2,
    "strict": 2,
    "no-extra-semi":1,
    "no-unreachable":2,
    "prefer-const":2,
    "no-const-assign":2
  }
}
